{
  "name": "@redot",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build": "turbo build --filter=@redot/ui @redot/utils @redot/sdui-renderer",
    "build:fast": "turbo build:fast --filter=@redot/ui @redot/utils @redot/sdui-renderer",
    "typecheck": "turbo typecheck --filter=@redot/*",
    "clean": "turbo clean --filter=@redot/*",
    "publish:github": "turbo build --filter=@redot/* && pnpm -r --filter @redot/* publish --no-git-checks",
    "lint": "eslint .",
    "format:check": "prettier --check .",
    "quality:check": "pnpm lint && pnpm format:check",
    "prepare": "husky",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "ci:publish": "pnpm publish -r",
    "release": "pnpm build && pnpm changeset:version && pnpm install && pnpm ci:publish"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --max-warnings 0",
      "prettier --check"
    ],
    "*.{js,jsx,json,css,md}": [
      "prettier --check"
    ]
  },
  "keywords": [],
  "author": "redot",
  "license": "ISC",
  "packageManager": "pnpm@10.6.1",
  "devDependencies": {
    "@changesets/cli": "^2.29.5",
    "@eslint/js": "^9.30.1",
    "@redot/sdui-renderer": "workspace:*",
    "@redot/themes": "workspace:*",
    "@redot/tokens": "workspace:*",
    "@redot/ui": "workspace:*",
    "@redot/utils": "workspace:*",
    "@storybook/addon-docs": "^9.0.17",
    "@storybook/addon-links": "^9.0.17",
    "@storybook/manager-api": "^8.6.14",
    "@storybook/react-vite": "^9.0.17",
    "@storybook/theming": "^8.6.14",
    "@tailwindcss/vite": "^4.1.11",
    "@types/node": "^24.0.15",
    "@types/react": "^19.1.8",
    "@vitejs/plugin-react": "^4.7.0",
    "clean-package": "^2.2.0",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "react": "^19.1.0",
    "storybook": "^9.0.17",
    "tailwindcss": "^4.1.11",
    "tsup": "^8.5.0",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.6"
  }
}
