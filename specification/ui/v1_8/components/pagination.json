{
  "name": "Pagination",
  "description": "페이지네이션 컴포넌트입니다.",
  "props": [
    {
      "name": "totalPages",
      "type": "number",
      "description": "전체 페이지 수"
    },
    {
      "name": "currentPage",
      "type": "number",
      "description": "현재 페이지"
    },
    {
      "name": "onPrevious",
      "type": "() => void",
      "description": "이전 버튼 클릭 콜백"
    },
    {
      "name": "onNext",
      "type": "() => void",
      "description": "다음 버튼 클릭 콜백"
    },
    {
      "name": "onPageChange",
      "type": "(page: number) => void",
      "description": "페이지 변경 콜백"
    },
    {
      "name": "hasNext",
      "type": "boolean",
      "default": "true",
      "description": "다음 페이지 존재 여부"
    },
    {
      "name": "hasPrevious",
      "type": "boolean",
      "default": "true",
      "description": "이전 페이지 존재 여부"
    }
  ],
  "usage": "```tsx\nimport { Pagination } from '@redotlabs/ui';\n\n<Pagination\n  totalPages={10}\n  currentPage={currentPage}\n  onPageChange={setCurrentPage}\n/>\n```"
}
